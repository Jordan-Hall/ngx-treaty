{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'authoring_treaty_authoring'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--lib",
					"--package=authoring_treaty_authoring"
				],
				"filter": {
					"name": "authoring_treaty_authoring",
					"kind": "lib"
				},
				,
				"env": {
					"RUST_BACKTRACE": "1",
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'rust_authoring'",
			"cargo": {
				"args": [
					"build",
					"--bin=rust_authoring",
					"--package=rust_authoring",
				],
				"filter": {
					"name": "rust_authoring",
					"kind": "bin"
				},
				"env": {
					"RUST_BACKTRACE": "1",
				}
			},
			"args": ["${workspaceFolder}/apps/rust/authoring/src/test.ts"],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'rust_authoring'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=rust_authoring",
					"--package=rust_authoring"
				],
				"filter": {
					"name": "rust_authoring",
					"kind": "bin"
				}
				,
				"env": {
					"RUST_BACKTRACE": "1",
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}